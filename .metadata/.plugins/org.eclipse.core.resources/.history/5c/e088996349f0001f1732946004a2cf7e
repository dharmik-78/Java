package com.itbulls.Dharmik;

abstract class ModifiersDemo {
    public String fname = "Dharmik";
    public int age = 22;
    public abstract void study();
}

class Student extends ModifiersDemo {
    public int graduationYear = 2020;

    public void study() {
        System.out.println("Studying all day long");
    }
}

public class Main {  // Fixed: Renamed class for the main method
    public static void main(String[] args) {
        Student myObj = new Student();
        
        System.out.println("Name: " + myObj.fname);
        System.out.println("Age: " + myObj.age);
        System.out.println("Graduation Year: " + myObj.graduationYear);
        
        myObj.study();
    }
}
