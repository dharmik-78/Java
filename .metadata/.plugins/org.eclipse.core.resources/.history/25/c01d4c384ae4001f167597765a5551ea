package Interface;

 interface Animal {
	public void animalSound();
	public void run();
	public void sleep();
}


class Pig implements Animal{
	public void animalSound() {
		System.out.println("The Pig Says : weeeeeee weeeee");
	}
	
	public void run() {
		System.out.println("Pig Can't run to fast..");
	}
	
	public void sleep() {
		System.out.println("Zzzzzzz");
	}
}


	class InterfaceDemo {
		public static void main(String[] args) {
			Pig myPig = new Pig();
			myPig.animalSound();
			myPig.sleep();
		}
	}