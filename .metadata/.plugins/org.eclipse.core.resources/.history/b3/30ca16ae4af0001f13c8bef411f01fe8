package inheritance;

import java.io.*;

// MULTILEVEL INHERITANCE

class One {
    public void print_greek() {
        System.out.println("Dharmik");
    }
}

class Two extends One {
    public void print_lastgreek() {  // Fixed spelling mistake
        System.out.println("Dharmik");
    }
}

// Missing class Three (Added)
class Three extends Two {
    public void print_for() {
        System.out.println("For");
    }
}

public class Multilevel {
    public static void main(String[] args) {
        Three g = new Three();
        
        g.print_greek();       // Inherited from One
        g.print_for();         // Defined in Three
        g.print_lastgreek();   // Inherited from Two
    }
}
